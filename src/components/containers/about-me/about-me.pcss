.aboutMe {
  @apply w-full self-center h-1/3 bg-gradient-to-r from-red-500 shadow-lg rounded-md mb-12 pb-2 mx-1;
  @apply md:mb-0 md:pb-0 md:ml-auto;
}

.aboutMe__inner {
  @apply container w-11/12 p-4 md:p-6 mx-auto md:ml-4 mt-4 bg-white shadow-lg rounded-md space-y-4;
  @apply xl:w-4/6 md:-mt-12;
}
